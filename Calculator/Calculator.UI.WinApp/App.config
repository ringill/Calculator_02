<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <connectionStrings>
    <!--Строка соединения с SQL хранилищем данных-->
    <add name="DefaultConnection" providerName="System.Data.SqlClient" connectionString="Data Source=(localdb)\Projects;Initial Catalog=Calculator.Infrastructure.SqlDatabase;Integrated Security=True;Connect Timeout=30;Encrypt=False;TrustServerCertificate=False;" />
  </connectionStrings>
  <appSettings>
    <!--Путь к файлу лога (xml)-->
    <add key="XmlFilePath" value="log.xml" />
  </appSettings>
</configuration>